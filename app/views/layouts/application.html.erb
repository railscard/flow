<!DOCTYPE html>
<html>
<head>
  <title>Flow</title>
  <%= csrf_meta_tags %>

  <%= stylesheet_pack_tag 'application' %>
  <%= javascript_pack_tag 'application' %>
</head>
<body>
<div class="container">
  <% flash.each do |key, value| %>
    <div class="alert alert-<%= key %>">
      <a href="#" data-dismiss="alert" class="close">Ã—</a>
      <ul>
        <li>
          <%= value %>
        </li>
      </ul>
    </div>
  <% end %>
  <%= yield %>
</div>
</body>
</html>
